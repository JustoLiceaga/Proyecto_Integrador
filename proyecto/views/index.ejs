<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->
<%- include("./partials/head") %>
<body>
	<!-- Header -->
	<%- include("./partials/header") %>
	<!-- /Header -->
	
	<!-- Banner -->
	<div class="banner">
		<img src="images/banner_kaye.png" alt="Banner de ofertas">
	</div>
	<!-- /Banner -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Productos</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<% producto.forEach(function(product) { %>
			<img src="<%= product.imagen %>" alt="foto de producto">
			<h3 > <%= product.nombre %></h3>
			<% }) %> 
				
			
		</div>
	</div>


	<!-- footer -->
  <%- include("./partials/footer") %>
	<!-- /footer -->
</body>	
</html>